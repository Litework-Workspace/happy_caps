    "165214717733bca1af": {
      "type": "image-with-text",
      "settings": {
        "image": "shopify://shop_images/Happy_Caps_-_Product_Image_3.png",
        "subtitle": "<span style=\"color : #D674A;\"> <h2> <span style=\"color : #DD674A;\"> Step 2 </span>\n</h2></span>",
        "title": "<h1>Lightly Mist</h1>",
        "text": "<p>Next, gently mist the mushroom block with water a few times a day to keep it nice and damp, after a day or two the mushrooms will begin to grow!</p>",
        "text_position": "right",
        "show_background": false,
        "expanded": false,
        "expanded_content": false,
        "collapsed": false,
        "show_on_desktop": true,
        "show_on_mobile": true
      }
